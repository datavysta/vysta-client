{
  "name": "@datavysta/vysta-client",
  "version": "1.0.42",
  "description": "TypeScript client for Vysta APIs",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "homepage": "https://www.datavysta.com/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/datavysta/vysta-client.git"
  },
  "bugs": {
    "url": "https://github.com/datavysta/vysta-client/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc",
    "prepare": "npm run build",
    "prepublishOnly": "if [ \"$SKIP_TESTS\" != \"true\" ]; then npm test; fi",
    "test": "NODE_OPTIONS=\"--experimental-vm-modules\" jest -c tests/jest.config.cjs",
    "test:watch": "NODE_OPTIONS=\"--experimental-vm-modules\" jest -c tests/jest.config.cjs --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@uppy/dashboard": "^4.3.2",
    "cross-fetch": "^4.0.0",
    "jwt-decode": "^4.0.0"
  },
  "peerDependencies": {
    "idb": "^8"
  },
  "optionalDependencies": {
    "idb": "^8.0.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "@uppy/core": "^4.4.2",
    "@uppy/tus": "^4.2.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.10",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^6.3.5"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "vysta",
    "datavysta",
    "api",
    "client",
    "typescript",
    "rest"
  ],
  "author": "dev@datavysta.com",
  "license": "MIT",
  "engines": {
    "node": ">=16"
  }
}
